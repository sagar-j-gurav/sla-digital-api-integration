{
  "name": "sla-digital-api-integration",
  "version": "2.0.0",
  "description": "End-to-end API integration for SLA Digital Alacrity platform with Express server and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node src/database/migrate.js",
    "migrate:rollback": "node src/database/migrate.js rollback",
    "migrate:reset": "node src/database/migrate.js reset",
    "test:zain": "node tests/test-zain-bahrain.js",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.js'"
  },
  "keywords": [
    "sla-digital",
    "carrier-billing",
    "dcb",
    "mobile-operators",
    "zain-bahrain",
    "webhook",
    "postgresql",
    "express"
  ],
  "author": "SLA Digital Integration Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagar-j-gurav/sla-digital-api-integration.git"
  },
  "bugs": {
    "url": "https://github.com/sagar-j-gurav/sla-digital-api-integration/issues"
  },
  "homepage": "https://github.com/sagar-j-gurav/sla-digital-api-integration#readme"
}